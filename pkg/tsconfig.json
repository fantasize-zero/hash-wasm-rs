{
  "compilerOptions": {
    "target": "es2022",
    "module": "ESNext",
    "sourceMap": true,
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "lib": ["esnext", "dom"],
    "moduleResolution": "Bundler"
  }
}
